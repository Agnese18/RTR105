#include <stdio.h>

int main ()
{
  unsigned char n;  
  printf ("ievadi naturalu skaitlii: ");
  scanf ("%hhd",&n);
  printf ("%d", (n&128)>>7);
  printf ("%d", (n&64)>>6);
  printf ("%d", (n&32)>>5);
  printf ("%d", ( n&16)>>4);
  printf ("%d", (n&8)>>3);
  printf ("%d", (n&4)>>2);
  printf ("%d",(n&2)>>1);
  printf ("%d",(n&1)>>0);
 
  
return 0;  

}
